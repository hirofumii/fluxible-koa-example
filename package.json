{
  "name": "fluxible-koa-example",
  "version": "1.3.0",
  "description": "A simple example of isomorphic Flux application using Koa, Fluxible, React and React-router",
  "main": "start.js",
  "scripts": {
    "build": "webpack & webpack --config webpack.config.production.js",
    "dev": "NODE_ENV=development nodemon start.js -e js,jsx",
    "start": "NODE_ENV=production node start.js"
  },
  "keywords": [
    "koa",
    "fluxible",
    "react",
    "react-rouer",
    "babel"
  ],
  "dependencies": {
    "babel-core": "^6.7.6",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "fluxible": "^1.0.5",
    "fluxible-addons-react": "^0.2.8",
    "fluxible-plugin-fetchr": "^0.3.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.0",
    "koa-static": "^2.0.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.1.1",
    "serialize-javascript": "^1.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-webpack": "^6.4.0",
    "json-loader": "^0.5.1",
    "nodemon": "^1.2.1",
    "webpack": "^2.1.0-beta.5"
  },
  "author": "hirofumii",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirofumii/fluxible-koa-example.git"
  },
  "engines": {
    "node": ">= 5.0"
  }
}
